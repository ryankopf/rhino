<%= turbo_frame_tag @block, class: "#{@block.css_classes.empty? ? 'm-4 p-4 card' : @block.css_classes} block block_#{@block.id}" do %>
  <%= render partial: 'rhino/blocks/form', block: @block %>
  <% for @block in @block.blocks %>
    <%= render partial: 'rhino/blocks/show', block: @block %>
  <% end %>
<% end %>